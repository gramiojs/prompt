		"question",
		"answer"
	],
	"devDependencies": {
		"@biomejs/biome": "^1.9.4",
		"@types/bun": "^1.2.13",
		"gramio": "^0.4.3",
		"pkgroll": "2.12.2",
		"typescript": "^5.8.3"
